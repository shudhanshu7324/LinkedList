public static Node<Integer> insertElement(Node<Integer> head,int data,int pos){
        Node<Integer> newNode = new Node<>(data);
        if (pos == 0){
            newNode.next = head;
            head = newNode;
            return head;
        }
        int i=0;
        Node temp = head;
        while(i<pos-1){
            temp = temp.next;
            i++;
        }
        newNode.next = temp.next;
        temp.next = newNode;

        return head;
    }
